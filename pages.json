{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
		    "path" : "pages/StartupPage/StartupPage",
		    "style": {
		    	"navigationBarTitleText": "",
		    	"navigationStyle": "custom"
		    }
		    
		}
		,{
			"path": "pages/index/index",
			"aliasPath": "/", //对于h5端你必须在首页加上aliasPath并设置为/
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": true,
				"navigationStyle": "custom",
				"mp-alipay": {
					"transparentTitle": "always",
					"titlePenetrate": "YES",
					"allowsBounceVertical": "NO"
				}
			},
			"app-plus": {
				"scrollIndicator": "none"
			}
		}
	    ,{
            "path" : "pages/login/login/login",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "登录",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/login/register/register",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "注册",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/myCoupon/myCoupon",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "我的优惠券",
                "enablePullDownRefresh": true
            }
            
        }
        ,{
            "path" : "pages/details/details",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "产品详情",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/myCoupon/myCoupon_notoken",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "我的优惠券",
                "enablePullDownRefresh": true
            }
            
        }
        ,{
            "path" : "pages/shop/shop",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "商家中心",
                "enablePullDownRefresh": true
            }
            
        }
        ,{
            "path" : "pages/shop/coupon_list/coupon_list",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "分类",
                "enablePullDownRefresh": true
            }
            
        }
        
        ,{
            "path" : "pages/webView/webView",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus":{
			"scrollIndicator":"none" //全局 在APP页面都不显示滚动条
		}
	},
	"uniIdRouter": {}
}
